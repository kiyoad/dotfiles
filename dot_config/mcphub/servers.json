{
  "mcpServers": {
    "arxiv-mcp-server": {
      "args": [
        "tool",
        "run",
        "arxiv-mcp-server",
        "--storage-path",
        "/Users/kiyoad/.arxiv-mcp-server-storage"
      ],
      "command": "uv",
      "disabled": false
    },
    "everything": {
      "args": [
        "-y",
        "@modelcontextprotocol/server-everything"
      ],
      "command": "npx",
      "disabled": false
    },
    "sequential-thinking": {
      "args": [
        "-y",
        "@modelcontextprotocol/server-sequential-thinking"
      ],
      "command": "npx",
      "disabled": false
    },
    "tavily": {
      "args": [
        "-y",
        "tavily-mcp@0.2.0"
      ],
      "command": "npx",
      "disabled": false,
      "env": {
        "TAVILY_API_KEY": "${TAVILY_API_KEY}"
      }
    }
  },
  "nativeMCPServers": {
    "neovim": {
      "disabled": true
    }
  }
}
