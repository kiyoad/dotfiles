{
  "nativeMCPServers": [
    
  ],
  "mcpServers": {
    "everything": {
      "args": [
        "-y",
        "@modelcontextprotocol/server-everything"
      ],
      "command": "npx",
      "disabled": false
    },
    "arxiv-mcp-server": {
      "args": [
        "tool",
        "run",
        "arxiv-mcp-server",
        "--storage-path",
        "/Users/kiyoad/.arxiv-mcp-server-storage"
      ],
      "command": "uv",
      "disabled": false
    },
    "tavily": {
      "env": {
        "TAVILY_API_KEY": "${input:tavily_api_key}"
      },
      "args": [
        "-y",
        "tavily-mcp@0.2.0"
      ],
      "command": "npx",
      "disabled": false
    },
    "sequential-thinking": {
      "args": [
        "-y",
        "@modelcontextprotocol/server-sequential-thinking"
      ],
      "command": "npx",
      "disabled": false
    }
  }
}